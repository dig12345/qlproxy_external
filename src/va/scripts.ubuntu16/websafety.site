server {

    listen 80;
    server_name websafety.example.lan;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /opt/websafety/var/console;
    }

    location / {
        include proxy_params;
        proxy_pass http://127.0.0.1:8000;
    }
}

